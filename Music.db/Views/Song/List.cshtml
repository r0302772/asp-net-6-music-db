@model List<Song>
@{
    ViewData["Title"] = "Songs";
}

<h1 class="text-center display-3">Songs</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                Title
            </th>
            <th scope="col">
                Genre
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var song in Model)
        {
            <tr>
                <td>
                    @song.Id
                </td>
                <td>
                    @song.Title
                </td>
                <td>
                    @song.Genre.Name
                </td>
            </tr>
        }
    </tbody>
</table>